<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/lsuxe/db" VERSION="20220629" COMMENT="XMLDB file for block blocks/lsuxe"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd">
  <TABLES>
    <TABLE NAME="block_lsuxe_moodles" COMMENT="The Moodle instances that have cross enrollments.">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="url" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" COMMENT="The URL for the Moodle instance"/>
        <FIELD NAME="token" TYPE="char" LENGTH="128" NOTNULL="true" SEQUENCE="false" COMMENT="The token"/>
        <FIELD NAME="tokenexpire" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="false" COMMENT="Token expiration date"/>
        <FIELD NAME="interval" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="Default update interval"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="The timestamp at which the record was modified. It is automatically set."/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="The timestamp at which the record was modified. It is automatically set, and defaults to 0."/>
        <FIELD NAME="timedeleted" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" COMMENT="The timestamp at which the record was deleted. It is automatically set, and defaults to 0."/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <!-- <KEY NAME="token" TYPE="foreign" FIELDS="token" REFTABLE="external_tokens" REFFIELDS="id"/> -->
      </KEYS>
    </TABLE>

    <TABLE NAME="block_lsuxe_mappings" COMMENT="Cross Enrollment mappings">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="course" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" COMMENT="Source course short name"/>
        <FIELD NAME="coursegroup" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" COMMENT="Source course group name"/>
        <FIELD NAME="destmoodleid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="ID for destination Moodle instance"/>
        <FIELD NAME="destcourse" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" COMMENT="Destination course short name"/>
        <FIELD NAME="destgroupprefix" TYPE="char" LENGTH="255" NOTNULL="true" DEFAULT="" SEQUENCE="false" COMMENT="Destination group prefix"/>
        <FIELD NAME="interval" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="Update interval, how often to run in hours"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="The timestamp at which the record was modified. It is automatically set."/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="The timestamp at which the record was modified. It is automatically set, and defaults to 0."/>
        <FIELD NAME="timedeleted" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false" COMMENT="The timestamp at which the record was deleted. It is automatically set, and defaults to 0."/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="destmoodleid" TYPE="foreign" FIELDS="destmoodleid" REFTABLE="xe_moodles" REFFIELDS="id"/>
      </KEYS>
    </TABLE>
    
    
  </TABLES>
</XMLDB>
